# Firebase Development Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Development Environment (maypole-flutter-dev)
FIREBASE_DEV_WEB_API_KEY=your_dev_web_api_key_here
FIREBASE_DEV_WEB_APP_ID=your_dev_web_app_id_here
FIREBASE_DEV_WEB_MEASUREMENT_ID=your_dev_web_measurement_id_here

FIREBASE_DEV_ANDROID_API_KEY=your_dev_android_api_key_here
FIREBASE_DEV_ANDROID_APP_ID=your_dev_android_app_id_here

FIREBASE_DEV_IOS_API_KEY=your_dev_ios_api_key_here
FIREBASE_DEV_IOS_APP_ID=your_dev_ios_app_id_here

FIREBASE_DEV_MESSAGING_SENDER_ID=your_dev_messaging_sender_id_here
FIREBASE_DEV_PROJECT_ID=maypole-flutter-dev
FIREBASE_DEV_AUTH_DOMAIN=maypole-flutter-dev.firebaseapp.com
FIREBASE_DEV_STORAGE_BUCKET=maypole-flutter-dev.firebasestorage.app

# Production Environment (maypole-flutter-ce6c3)
FIREBASE_PROD_WEB_API_KEY=your_prod_web_api_key_here
FIREBASE_PROD_WEB_APP_ID=your_prod_web_app_id_here
FIREBASE_PROD_WEB_MEASUREMENT_ID=your_prod_web_measurement_id_here

FIREBASE_PROD_ANDROID_API_KEY=your_prod_android_api_key_here
FIREBASE_PROD_ANDROID_APP_ID=your_prod_android_app_id_here

FIREBASE_PROD_IOS_API_KEY=your_prod_ios_api_key_here
FIREBASE_PROD_IOS_APP_ID=your_prod_ios_app_id_here

FIREBASE_PROD_WINDOWS_APP_ID=your_prod_windows_app_id_here
FIREBASE_PROD_WINDOWS_MEASUREMENT_ID=your_prod_windows_measurement_id_here

FIREBASE_PROD_MESSAGING_SENDER_ID=your_prod_messaging_sender_id_here
FIREBASE_PROD_PROJECT_ID=maypole-flutter-ce6c3
FIREBASE_PROD_AUTH_DOMAIN=maypole-flutter-ce6c3.firebaseapp.com
FIREBASE_PROD_STORAGE_BUCKET=maypole-flutter-ce6c3.firebasestorage.app

# Google Places API
GOOGLE_PLACES_DEV_API_KEY=your_dev_google_places_api_key_here
GOOGLE_PLACES_PROD_API_KEY=your_prod_google_places_api_key_here

# App Configuration
IOS_BUNDLE_ID=app.maypole.maypole

# Build Environment (dev or production)
ENVIRONMENT=dev